{
  "name": "www.somarl.com",
  "homepage": "https://somarl.com",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start-js": "node scripts/start.js",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "node scripts/build.js",
    "build": "npm-run-all build-css build-js",
    "build-css": "node-sass-chokidar --include-path ./node_modules ./src/scss -o ./src/css",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./node_modules ./src/scss -o ./src/css --watch --recursive",
    "test": "node scripts/test.js",
    "ci": "node scripts/test.js --ci --coverage",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "bootstrap": "^4.1.1",
    "object-assign": "4.1.1",
    "promise": "8.0.1",
    "react": "16.6.3",
    "react-dom": "16.6.3",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "@types/jest": "^23.0.0",
    "@types/react": "16.7.11",
    "@types/react-dom": "16.0.10",
    "folans.d.ts": "^1.1.0",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "typescript": "^3.0.3",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "autoprefixer": "7.1.6",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "raf": "3.4.0",
    "react-dev-utils": "^5.0.2",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.7",
    "resolve": "1.6.0",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "source-map-loader": "^0.2.1",
    "style-loader": "0.19.0",
    "url-loader": "0.6.2",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fork-ts-checker-webpack-plugin": "^0.2.8",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "ts-jest": "22.0.1",
    "ts-loader": "^2.3.7",
    "tsconfig-paths-webpack-plugin": "^2.0.0",
    "tslint": "^5.7.0",
    "tslint-react": "^3.2.0",
    "uglifyjs-webpack-plugin": "1.2.5",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.11.3",
    "webpack-manifest-plugin": "1.3.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "babel-preset-react-app": "^3.1.2"
  }
}
